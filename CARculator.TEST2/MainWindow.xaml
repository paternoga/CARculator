<Window x:Class="CARculator.TEST2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Car Info" Height="400" Width="600">
    <Grid>
        <ComboBox x:Name="GenerationComboBox" HorizontalAlignment="Left" Margin="10" VerticalAlignment="Top" Width="200" SelectionChanged="GenerationComboBox_SelectionChanged"/>
        <ComboBox x:Name="EngineComboBox" HorizontalAlignment="Left" Margin="10,40,0,0" VerticalAlignment="Top" Width="200" SelectionChanged="EngineComboBox_SelectionChanged" IsEnabled="False"/>
        <TextBox x:Name="CarInfoTextBox" HorizontalAlignment="Left" Margin="10,80,0,0" VerticalAlignment="Top" Width="560" Height="250" TextWrapping="Wrap" IsReadOnly="True"/>
    </Grid>
</Window>
